{
  "name": "youth-portal-mono-repo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "kill-port 5001 5173 && concurrently \"yarn dev:client\" \"yarn dev:api\"",
    "dev:client": "vite client --mode development --port 5173",
    "dev:api": "nodemon",
    "build:client:dev": "vite build client --mode development",
    "build:client:prod": "vite build client --mode production",
    "build:server": "tsc -p api/tsconfig.build.json",
    "build:dev": "yarn build:client:dev && ts-node scripts/copy-ui-build.ts && yarn build:server",
    "build:prod": "yarn build:client:prod && ts-node scripts/copy-ui-build.ts && yarn build:server",
    "start": "node api/dist/server.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@aws-sdk/client-s3": "^3.978.0",
    "@aws-sdk/credential-providers": "^3.978.0",
    "@aws-sdk/s3-request-presigner": "^3.978.0",
    "@tanstack/react-query": "^5.0.0",
    "antd": "^5.10.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.7.0",
    "firebase-admin": "^13.6.0",
    "mongodb": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^11.0.3",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "fs-extra": "^11.1.1",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
